{
  "name": "brdgme-web",
  "version": "0.1.0",
  "description": "Web interface for brdg.me",
  "repository": "https://github.com/brdgme/web.git",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "build-prod": "node_modules/.bin/webpack -p",
    "clean": "rm -rf dist test-build",
    "test": "node_modules/.bin/tsc --outDir test-build test/test.ts && node_modules/.bin/mocha test-build/test.js",
    "watch": "node_modules/.bin/webpack -w",
    "server": "node server.js",
    "dev": "npm run watch & npm run server & npm run websocket",
    "dev-html": "npm run watch & npm run server",
    "websocket": "cd node_modules/brdgme-websocket-server && npm run start-dev"
  },
  "author": "Michael Alexander <beefsack@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.13",
    "@types/redux-immutable": "^3.0.38",
    "@types/superagent": "^3.5.6",
    "classnames": "^2.2.5",
    "immutable": "^4.0.0-rc.9",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "superagent": "^3.8.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "awesome-typescript-loader": "^3.4.0",
    "brdgme-websocket-server": "git+https://github.com/brdgme/websocket-server.git",
    "chai": "^4.0.0",
    "css-loader": "^0.28.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.2",
    "mocha": "^4.0.1",
    "moment": "^2.19.2",
    "morgan": "^1.9.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "tslint": "^5.8.0",
    "typescript": "2.6.1",
    "webpack": "^3.8.1"
  }
